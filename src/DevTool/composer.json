{
  "name": "spaceonfire/dev-tool",
  "description": "",
  "type": "library",
  "keywords": [],
  "homepage": "https://github.com/spaceonfire/spaceonfire",
  "license": "MIT",
  "authors": [
    {
      "name": "Constantine Karnaukhov",
      "email": "genteelknight@gmail.com",
      "homepage": "https://www.onfire.space",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "composer/semver": "^3.0",
    "spaceonfire/criteria": "^3.0",
    "spaceonfire/type": "^3.0",
    "webmozart/assert": "^1.6"
  },
  "require-dev": {
    "gitonomy/gitlib": "^1.2",
    "roave/security-advisories": "dev-latest"
  },
  "bin": [
    "bin/dev-tool"
  ],
  "autoload": {
    "psr-4": {
      "spaceonfire\\DevTool\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spaceonfire\\DevTool\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
